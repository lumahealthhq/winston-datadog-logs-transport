{
  "name": "winston-datadog-logs-transport",
  "version": "1.0.7",
  "description": "A Datadog Logs transport for Winston",
  "license": "MIT",
  "repository": "lumahealthhq/winston-datadog-logs-transport",
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "test": "echo ok"
  },
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "keywords": [
    "logging",
    "logs",
    "logger",
    "winston",
    "datadog",
    "datadog logs"
  ],
  "dependencies": {
    "dd-trace": "^0.17.0",
    "fast-safe-stringify": "^2.0.7",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@shelf/eslint-config": "0.12.1",
    "eslint": "6.7.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "cross-env npm run commitmsg $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
